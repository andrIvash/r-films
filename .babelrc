{
  "presets": [
    ["env", { 
      "modules": false,
      "targets": {
        "browsers": ["last 2 versions", "ie >= 11"]
      }
    }],
    "react",
    "stage-0",
    "flow"
  ],
  "plugins": [
    "transform-class-properties"
  ],
  "env": {
    "test": {
      "presets": [["env"], "stage-0", "react", "flow"]
    }
  }
}